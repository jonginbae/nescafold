<p id="notice"><%= notice %></p>

<table id="table" class="table table-striped">
  <tr id="table-header">
    <th>Name</th>
    <th>Price</th>
    <th>Quantity</th>
    <th>Status</th>
  </tr>
  <tr>
    <th>total :</th>
    <th class="total"></th>
    <th></th>
    <th></th>
  </tr>
</table>
<hr>
  <div class="row">
    <div class="col-md-5">
      <h1>Admin Main Page</h1>
      <table class="table table-striped">
        <tr>
          <th>Name</th>
          <th>Status</th>
          <th colspan="3"></th>
        </tr>
        <% @orders.each do |order| %>
          <tr>
            <td class="order_name"><%= order.name %></td>
            <td class="status"><%= order.status %></td>
          </tr>
        <% end %>
      </table>
    </div>
    <div class="col-md-2">
    </div>
    <div class="col-md-3">
      <div class="cont">
      <progress id="progress-bar" min="1" max="100" value="0"></progress>
      <span id="progress" class="first border-change"></span>
      <span id="progress" class="second"></span>
      <span id="progress" class="third"></span>
      <span id="progress" class="fourth"></span>
      <p class="percent">0% Complete</p>
      </div>
    </div>
  </div>
